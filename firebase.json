{
  "hosting": [
    {
      "predeploy": ["npm --prefix hosting run build:prod"],
      "target": "roesena-app",
      "public": "hosting/dist/roesena-app",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "functions": {
    "predeploy": ["npm --prefix functions run lint", "npm --prefix functions run build"]
  }
}
